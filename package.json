{
    "name": "portfolio.peresonal.com",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "dev": "bunx --bun vite dev",
        "build": "vite build",
        "preview": "bunx --bun vite preview",
        "check": "bunx --bun svelte-kit sync && bunx --bun svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "bunx --bun svelte-kit sync && bunx --bun svelte-check --tsconfig ./tsconfig.json --watch",
        "lint": "prettier --check . && bunx --bun eslint .",
        "format": "prettier --write ."
    },
    "devDependencies": {
        "@catppuccin/tailwindcss": "^0.1.6",
        "@sveltejs/adapter-auto": "^3.0.0",
        "@sveltejs/kit": "^2.0.0",
        "@sveltejs/vite-plugin-svelte": "^3.0.0",
        "@types/eslint": "8.56.0",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "autoprefixer": "^10.4.17",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-svelte": "^2.35.1",
        "postcss": "^8.4.34",
        "prettier": "^3.1.1",
        "prettier-plugin-svelte": "^3.1.2",
        "svelte": "^4.2.7",
        "svelte-adapter-azure-swa": "^0.20.0",
        "svelte-check": "^3.6.0",
        "tailwindcss": "^3.4.1",
        "tslib": "^2.4.1",
        "typescript": "^5.0.0",
        "vite": "^5.0.3"
    },
    "type": "module",
    "dependencies": {
        "lucide-svelte": "^0.330.0"
    },
    "engines": {
        "node": ">=18.13 <19"
    }
}
