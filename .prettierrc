{
    "useTabs": false,
    "singleQuote": false,
    "trailingComma": "all",
    "endOfLine": "lf",
    "arrowParens": "always",
    "semi": true,
    "tabWidth": 4,
    "printWidth": 96,
    "plugins": ["prettier-plugin-svelte"],
    "overrides": [
        {
            "files": "*.svelte",
            "options": {
                "parser": "svelte"
            }
        }
    ]
}
